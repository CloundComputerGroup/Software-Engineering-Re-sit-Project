@*@using GHC.BPP.Model;
@using GHC.BPP.Web.Common;*@
<!DOCTYPE html>
<html id="htmlRoot" ng-app="bankApp">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, shrink-to-fit=no, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Cloud Computer Bank</title>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/bank_controllers")
    <script src="~/Scripts/bankScript/ctrl-admin.js"></script>
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/templatebootstrap")

    @Styles.Render("~/Content/templatecss")
    <link href="~/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <script type="text/javascript" src="~/js/locales/bootstrap-datetimepicker.fr.js"></script>
    <script type="text/javascript" src="~/js/bootstrap-datetimepicker.js"></script>
    <script src="~/Scripts/template/datatables/jquery.dataTables.js"></script>
    <script>
        $(document).ready(function () {
            $('.form_datetime').datetimepicker({
                //language:  'fr',
                weekStart: 1,
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                startView: 2,
                forceParse: 0,
                showMeridian: 1
            });
            $('.form_date').datetimepicker({
                language: 'uk',
                weekStart: 1,
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                startView: 2,
                minView: 2,
                forceParse: 0
            });
            $('.form_time').datetimepicker({
                language: 'uk',
                weekStart: 1,
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                startView: 1,
                minView: 0,
                maxView: 1,
                forceParse: 0
            });
        });
    </script>
    <!-- Bootstrap Core CSS -->

    @RenderSection("Scripts", required: false)
    @RenderSection("Styles", required: false)

    <!-- Custom CSS -->
</head>
<body ng-controller="bankCtrl" id="bankCtrl">
    <div class="navbar navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".navbar-inverse-collapse">
                    <i class="icon-reorder shaded"></i>
                </a><a class="brand" href="#">Cloud Computer Bank </a>
                <div class="nav-collapse collapse navbar-inverse-collapse">
                    <ul class="nav nav-icons">
                        <li class="active"><a href="#"><i class="icon-envelope"></i></a></li>
                        <li><a href="#"><i class="icon-eye-open"></i></a></li>
                        <li><a href="#"><i class="icon-bar-chart"></i></a></li>
                    </ul>
                    <form class="navbar-search pull-left input-append" action="#">
                        <input type="text" class="span3">
                        <button class="btn" type="button">
                            <i class="icon-search"></i>
                        </button>
                    </form>
                    <ul class="nav pull-right">
                       
                        <li><a href="#">@ViewBag.UserName </a></li>
                        <li class="nav-user dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/images/user.png" class="nav-avatar" />
                                <b class="caret"></b>
                            </a>
                            <ul class="dropdown-menu">
                                <li><a href="#">Profile</a></li>
                                <li><a href="#">Edit Profile</a></li>
                                <li class="divider"></li>
                                <li><a ng-click="loadSignIn()"  href="#">Logout</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!-- /.nav-collapse -->
            </div>
        </div>
        <!-- /navbar-inner -->
    </div>
    <!-- /navbar -->
    <div class="wrapper">
        <div class="container">
            <div class="row">
                <div class="span3">
                    <div class="sidebar">
                        <ul class="widget widget-menu unstyled">
                            <li class="active">
                                <a ng-click="loadDashboard()" href="">
                                    <i class="menu-icon icon-dashboard"></i>Dashboard
                                </a>
                            </li>
                            <li>
                                <a ng-click="loadAccountType()" href=""><i class="menu-icon icon-tasks"></i>Account Types </a>
                            </li>
                            <li>
                                <a ng-click="loadAccount()" href="">
                                    <i class="menu-icon icon-tasks"></i>Accounts
                                </a>
                            </li>
                            <li>
                                <a ng-click="loadTransaction()" href="">
                                    <i class="menu-icon icon-tasks"></i>Transaction History
                                </a>
                            </li>
                            <li>
                                <a ng-click="loadRecurringPayment()" href="">
                                    <i class="menu-icon icon-tasks"></i>Recurring Payments
                                </a>
                            </li>
                        </ul>
                        <ul class="widget widget-menu unstyled">
                            <li>
                                <a class="collapsed" data-toggle="collapse" href="#togglePages">
                                    <i class="menu-icon icon-cog">
                                    </i><i class="icon-chevron-down pull-right"></i><i class="icon-chevron-up pull-right">
                                    </i>Mannage Services
                                </a>
                                <ul id="togglePages" class="collapse unstyled">
                                    <li><a  ng-click="debitcreditCardLoad()" href=""><i class="icon-inbox"></i>Debit/Credit Card </a></li>
                                    <li><a ng-click="chequebookLoad()" href=""><i class="icon-inbox"></i>Cheque/Chequebook </a></li>
                                    @*<li><a href="other-user-listing.html"><i class="icon-inbox"></i>All Users </a></li>*@
                                </ul>
                            </li>
                            <li><a ng-click="loadSignIn()" href="#"><i class="menu-icon icon-signout"></i>Logout </a></li>
                        </ul>
                        <!--/.widget-nav-->


                       
                    </div>
                    <!--/.sidebar-->
                </div>
                <!--/.span3-->
                <div class="span9">

                    @RenderBody()
                </div>
                <!--/.span9-->
            </div>
        </div>
        <!--/.container-->
    </div>
    <!--/.wrapper-->
    <div class="footer">
        <div class="container">
            <b class="copyright">&copy; 2024 Software Engineering Bank Project </b>All rights reserved.
        </div>
    </div>
    <!-- jQuery -->
    @*<script src="~/js/jquery.js"></script>*@
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jqueryval")


    @Scripts.Render("~/bundles/results")
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script src="~/js/CircleChart.js" type="application/javascript"></script>

    <!-- Bootstrap Core JavaScript -->
    @*<script src="~/js/bootstrap.min.js"></script>*@
    <script src='~/js/moment.min.js'></script>
    @*<script src='~/js/jquery.min.js'></script>*@
    <script src='~/js/fullcalendar.min.js'></script>
   

    <script>
        $(function () {
            $("#datepicker").datepicker();
        });
    </script>



    <!-- Menu Toggle Script -->
    <script>
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });
    </script>
   

   

   

  


 

   


</body>
</html>
