@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var emptyGuidId = "00000000-0000-0000-0000-000000000000";
}
<div class="content" >
    <div class="btn-controls">
        <div class="btn-box-row row-fluid">
            <a href="#" class="btn-box big span4">
                <i class=" icon-random"></i><b>@ViewBag.CreditAmount</b>
                <p class="text-muted">
                    Credit Transactions
                </p>
            </a>
            <a href="#" class="btn-box big span4">
                <i class=" icon-random"></i><b>@ViewBag.DebitAmount</b>
                <p class="text-muted">
                    Debit Transactions
                </p>
            </a>
            <a href="#" class="btn-box big span4">
                <i class=" icon-random"></i><b>@ViewBag.TotalAmount</b>
                <p class="text-muted">
                    Current Balance
                </p>
            </a>
           
        </div>
        <div class="btn-box-row row-fluid">

            <a href="#" class="btn-box big span4">
                <i class="icon-user"></i><b>@ViewBag.NumberOfAccount</b>
                <p class="text-muted">
                    Number of Active Account
                </p>
            </a>
            <a href="#" class="btn-box big span4">
                <i class="icon-money"></i><b>@ViewBag.BusinessBalance</b>
                <p class="text-muted">
                    Business Account Balance
                </p>
            </a>
            <a href="#" class="btn-box big span4">
                <i class="icon-money"></i><b>@ViewBag.PersonalBalance</b>
                <p class="text-muted">
                    Personal Account Balance
                </p>
            </a>
        </div>
        <div class="btn-box-row row-fluid">
            <div class="span12">
                <div class="row-fluid">
                    <a ng-click="loadTransaction()" href="#" class="btn-box small span3">
                        <i class="icon-exchange"></i><b>Deposit</b>
                    </a>
                    <a ng-click="loadTransaction()" href="#" class="btn-box small span3">
                        <i class="icon-exchange"></i><b>Transfers</b>
                    </a>
                    <a ng-click="loadTransaction()" href="#" class="btn-box small span3">
                        <i class="icon-exchange"></i><b>Withdraw</b>
                    </a>
                    <a ng-click="loadTransaction()" href="#" class="btn-box small span3">
                        <i class="icon-exchange"></i><b>Payment</b>
                    </a>
                </div>
               
            </div>
